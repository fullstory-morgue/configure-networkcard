#: netcardconfig:24
msgid "Error: You must be root to run this script!"
msgstr ""

#: netcardconfig:229
#, sh-format
msgid ""
"Enter the ESSID for ${DEVICENAME} \\n\\n\\n(empty for 'any', not "
"recommended !)\\n"
msgstr "Inserisci l'ESSID per ${DEVICENAME}\\n\\n\\n(vuoto per 'tutti', non raccomandato!)\\n"

#: netcardconfig:231
#, sh-format
msgid "Enter the NWID (cell identifier)\\nfor ${DEVICENAME}, if needed\\n\\n\\n"
msgstr "Inserisci il NWID (cell identifier)\\nper ${DEVICENAME}, se necessario\\n\\n\\n"

#: netcardconfig:233
#, sh-format
msgid ""
"Enter the mode for ${DEVICENAME}\\n\\n(Managed(=default), Ad-Hoc, Master,"
"\\nRepeater, Secondary, auto)\\n"
msgstr "Inserisci la modalità per ${DEVICENAME}\\n\\n(Managed(=default), Ad-Hoc, Master,\\nRepeater, Secondary, auto)\\n"

#: netcardconfig:235
#, sh-format
msgid ""
"Enter channel number for ${DEVICENAME} \\n\\n(0 to 16, empty for auto or if "
"you want to\\n enter the frequency next)\\n"
msgstr "Inserisci il numero di canale per ${DEVICENAME}\\n\\n(da 0 a 16, vuoto per automatico o se vuoi\\n inserire la frequnza dopo)\\n"

#: netcardconfig:238
#, sh-format
msgid "Enter the frequency for ${DEVICENAME} \\n\\n(e.g 2.412G, empty for auto)"
msgstr "Inserisci la frequenza per ${DEVICENAME}\\n\\n(es. 2.412G, vuoto per automatico)"

#: netcardconfig:263
msgid "Enable WPA support?"
msgstr "Abilito il supporto WPA?"

#: netcardconfig:296
#, sh-format
msgid "Enter the WPA Passphrase\\nfor ${ESSID}"
msgstr "Inserisci la Passphrase di WPA\\nper ${ESSID}"

#: netcardconfig:312
#, sh-format
msgid ""
"Enter the encryption key\\nfor ${DEVICENAME} \\n\\n(empty for cleartext, not "
"recommended!!)"
msgstr "Inserisci la chiave crittografica\\nper ${DEVICENAME}\\n\\n(vuoto per trasmissione in chiaro, non raccomandato!)"

#: netcardconfig:315
#, sh-format
msgid ""
"Would you like to store your wep key in it's own private file "
"(${DEVICENAME})? If you say no, your wep key will be stored in /etc/network/"
"interfaces and will be readable by any account on your system.  You may want "
"to 'chmod 700 /etc/network/interfaces' if you answer no to this question"
msgstr "Vuoi memorizzare la tua chiave WEP nel suo file riservato (${DEVICENAME})?   Se dici no, la tua chiave WEP sarà memorizzata nel file /etc/network/interfaces e sarà leggibile da tutti gli account del tuo sistema. Dovresti fare 'chmod 700 /etc/network/interfaces' se rispondi no a questa domanda"

#: netcardconfig:321
#, sh-format
msgid ""
"Enter additional parameters for\\n'iwconfig ${DEVICENAME}' if needed, e.g.\\n"
"\\n\\nsens -80  rts 512  frag 512  rate 5.5M"
msgstr "Inserisci i parametri aggiuntivi per\\n'iwconfig ${DEVICENAME}' se necessario, es.\\n\\n\\nsens -80  rts 512  frag 512  rate 5.5M"

#: netcardconfig:323
#, sh-format
msgid "Enter additional parameters for\\n'iwspy ${DEVICENAME}' if needed\\n\\n\\n"
msgstr "Inserisci parametri aggiuntivi per\\n'iwspy ${DEVICENAME}' se necessario\\n\\n\\n"

#: netcardconfig:325
#, sh-format
msgid "Enter additional parameters for\\n'iwpriv ${DEVICENAME}' if needed\\n\\n\\n"
msgstr "Inserisci parametri aggiuntivi per\\n'iwpriv ${DEVICENAME}' se necessario\\n\\n\\n"

#: netcardconfig:330
msgid "Use DHCP broadcast?"
msgstr "Devo utilizzare il broadcast DHCP?"

#: netcardconfig:358
#, sh-format
msgid "Please enter IP Address for ${DV}"
msgstr "Inserisci l'indirizzo IP per ${DV}"

#: netcardconfig:360
#, sh-format
msgid "Please enter Network Mask for ${DV}"
msgstr "Inserisci la maschera di rete per ${DV}"

#: netcardconfig:362
#, sh-format
msgid "Please enter Broadcast Address for ${DV}"
msgstr "Inserisci l'indirizzo di broadcast per ${DV}"

#: netcardconfig:364
msgid "Please enter Default Gateway"
msgstr "Inserisci il Gateway di default"

#: netcardconfig:370
msgid "Please enter Nameserver(s)"
msgstr "Inserisci i Nameserver"

#: netcardconfig:482
msgid "No supported network cards found."
msgstr "Non ho trovato schede di rete supportate."

#: netcardconfig:518
msgid "Device"
msgstr ""

#: netcardconfig:518
msgid "Please select network device"
msgstr "Per favore seleziona la periferica di rete"

#: netcardconfig:539
msgid "Failed to bring up the interface, would you like to reconfigure it?"
msgstr "Non riesco ad attivare l'interfaccia, vuoi riconfigurarla?"

#: netcardconfig:541
msgid "Interface enabled, do you want it auto enabled at boot?"
msgstr "Interfaccia attiva, vuoi abilitarla automaticamente all'avvio?"

