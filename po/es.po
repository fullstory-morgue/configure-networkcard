#: netcardconfig:24
msgid "Error: You must be root to run this script!"
msgstr ""

#: netcardconfig:229
#, sh-format
msgid ""
"Enter the ESSID for ${DEVICENAME} \\n\\n\\n(empty for 'any', not "
"recommended !)\\n"
msgstr "Teclee el ESSID para ${DEVICENAME} \\n\\n\\n(vacío para 'cualquiera', ¡no recomendado!)\\n"

#: netcardconfig:231
#, sh-format
msgid "Enter the NWID (cell identifier)\\nfor ${DEVICENAME}, if needed\\n\\n\\n"
msgstr "Teclee el NWID (identificador de celda)\\npara ${DEVICENAME}, si es necesario\\n\\n\\n"

#: netcardconfig:233
#, sh-format
msgid ""
"Enter the mode for ${DEVICENAME}\\n\\n(Managed(=default), Ad-Hoc, Master,"
"\\nRepeater, Secondary, auto)\\n"
msgstr "Teclee el modo para ${DEVICENAME}\\n\\n(Managed(es=por_defecto), Ad-Hoc, Master,\\nRepeater, Secondary, auto)\\n"

#: netcardconfig:235
#, sh-format
msgid ""
"Enter channel number for ${DEVICENAME} \\n\\n(0 to 16, empty for auto or if "
"you want to\\n enter the frequency next)\\n"
msgstr "Teclee el número del canal para ${DEVICENAME}\\n\\n(0 bis 16, vacío para auto o si lo prefiere\\n teclee la frecuencia seguidamente)\\n"

#: netcardconfig:238
#, sh-format
msgid "Enter the frequency for ${DEVICENAME} \\n\\n(e.g 2.412G, empty for auto)"
msgstr "Teclee la frecuencia para ${DEVICENAME}\\n\\n(ej. 2.412G, vacío para auto)"

#: netcardconfig:263
msgid "Enable WPA support?"
msgstr ""

#: netcardconfig:296
#, sh-format
msgid "Enter the WPA Passphrase\\nfor ${ESSID}"
msgstr ""

#: netcardconfig:312
#, sh-format
msgid ""
"Enter the encryption key\\nfor ${DEVICENAME} \\n\\n(empty for cleartext, not "
"recommended!!)"
msgstr "Teclee la clave de encriptación\\npara ${DEVICENAME}\\n\\n(vacío para texto plano, ¡¡no recomendado!!)"

#: netcardconfig:315
#, sh-format
msgid ""
"Would you like to store your wep key in it's own private file "
"(${DEVICENAME})? If you say no, your wep key will be stored in /etc/network/"
"interfaces and will be readable by any account on your system.  You may want "
"to 'chmod 700 /etc/network/interfaces' if you answer no to this question"
msgstr ""

#: netcardconfig:321
#, sh-format
msgid ""
"Enter additional parameters for\\n'iwconfig ${DEVICENAME}' if needed, e.g.\\n"
"\\n\\nsens -80  rts 512  frag 512  rate 5.5M"
msgstr "Teclee los parámetros adicionales para\\n'iwconfig ${DEVICENAME}' Si es necesario, ej.\\n\\n\\nsens -80  rts 512  frag 512  rate 5.5M"

#: netcardconfig:323
#, sh-format
msgid "Enter additional parameters for\\n'iwspy ${DEVICENAME}' if needed\\n\\n\\n"
msgstr "Teclee los parámetros adicionales para\\n'iwspy ${DEVICENAME}' si es necesario\\n\\n\\n"

#: netcardconfig:325
#, sh-format
msgid "Enter additional parameters for\\n'iwpriv ${DEVICENAME}' if needed\\n\\n\\n"
msgstr "Teclee los parámetros adicionales para\\n'iwpriv ${DEVICENAME}' si es necesario\\n\\n\\n"

#: netcardconfig:330
msgid "Use DHCP broadcast?"
msgstr "¿Utilizar broadcast DHCP?"

#: netcardconfig:358
#, sh-format
msgid "Please enter IP Address for ${DV}"
msgstr "Por favor, inserte la dirección IP para ${DV}"

#: netcardconfig:360
#, sh-format
msgid "Please enter Network Mask for ${DV}"
msgstr "Por favor, inserte la máscara de red para ${DV}"

#: netcardconfig:362
#, sh-format
msgid "Please enter Broadcast Address for ${DV}"
msgstr "Por favor, inserte la dirección de Broadcast para ${DV}"

#: netcardconfig:364
msgid "Please enter Default Gateway"
msgstr "Por favor, inserte la puerta de enlace por defecto"

#: netcardconfig:370
msgid "Please enter Nameserver(s)"
msgstr "Por favor, inserte los servidores DNS"

#: netcardconfig:482
msgid "No supported network cards found."
msgstr "Se han encontrado tarjetas de red no soportadas."

